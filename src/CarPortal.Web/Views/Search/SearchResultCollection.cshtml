@using CarPortal.Web.Models.Offer
@model List<OfferInCollectionViewModel>

@{
    if (Model.Count == 0)
    {
        <h3 class="text-center">There are no offers that match your criteria.</h3>
    }
    else
    {
        ViewBag.Title = "Search results:";
        <partial name="_OfferCollectionPartial"></partial>
    }
}


